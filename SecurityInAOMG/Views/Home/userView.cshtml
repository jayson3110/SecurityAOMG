@model IEnumerable<SecurityInAOMG.Models.Artist>

@{
    Layout = "~/Views/Shared/_View.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>userView</title>
</head>
<body>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Artist_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Artist_role)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Artist_contact)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Artist_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Artist_role)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Artist_contact)
                </td>

            </tr>
        }

    </table>

    <a href="@Url.Action("Logout","Login")" class="btn btn-warning">Logout</a>
</body>
</html>
